<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="style.css" />
  </head>
  <body>
    <div class="content">
      <section class="container">
        <h1><a name="top"></a>Frumpy</h1>
          <ul>
            {{#entries}}
              <li><a href="#{{id}}">{{id}}</a></li>
            {{/entries}}
          </ul>

        {{#entries}}
          <section>
            <a class="pull-right" href="#top">[top]</a>
            <h3><a name="{{id}}"></a>{{id}}</h3>
            <section>{{{description_html}}}</section>
            <h5>Arguments</h5>
            <table>
              <tr>
                <th>Type</th>
                <th>Name</th>
                <th>Description</th>
              </tr>
            {{#params}}
              <tr>
                <td><code>{{type}}</code></td>
                <td>{{name}}</td>
                <td>{{description}}</td>
              </tr>
            {{/params}}
            </table>
            {{#example}}
              <h5>Example</h5>
              <pre><code>{{example}}</code></pre>
            {{/example}}

          </section>
        {{/entries}}
      </section>
    </div>
    <footer class="dark">
      <p class="copyright">
        Another unrepentant production from <a href="http://rjzaworski.com">rj zaworski</a>
      </p>
    </footer>
  </body>
</html>

